class Solution {
    int getCount(Node root, int l, int h) {
        // Your code here
        if(root==null)
        {
            return 0;
        }
        int count=0;
        if(root.data>=l && root.data<=h)
        {
            count=1;
        }
        return count + getCount(root.left,l,h) + getCount(root.right,l,h);

        
    }
}
