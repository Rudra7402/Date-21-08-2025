class Solution {
    public TreeNode insertIntoBST(TreeNode root, int key) {
       if(root==null)
        {
            return new TreeNode(key);
        }
        
        if(key<root.val)
        {
            root.left=insertIntoBST(root.left,key);
        }
        else if(key>root.val)
        {
            root.right=insertIntoBST(root.right,key);
        }
        return root; 
    }
}
